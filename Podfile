# platform :ios, '8.0'
# Uncomment this line if you're using Swift
use_frameworks!

target 'NotifyHub' do
    pod 'Alamofire', '~> 3.3'
    pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
end

plugin 'cocoapods-keys', {
  :project => "NotifyHub",
  :keys => [
    "GitHubClientSecret",
    "GitHubClientId"
]}
